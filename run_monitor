pids=$(ps -ef|grep gunicorn|grep run_monitor|awk {'print $2,$3'})
pidsArray=($pids)

len=${#pidsArray[*]}
for ((index=0;index<$len;index+=1));do
    tmp=`expr $index+1`
    echo ${pidsArray[$index]}
    kill -9 ${pidsArray[$index]}
done
sleep 1
gunicorn --worker-class=gevent -w 4 -b 0.0.0.0:7259 run_monitor:app
